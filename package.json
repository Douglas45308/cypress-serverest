{
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "allure-commandline": "^2.34.0",
    "cypress": "^14.4.0"
  },
  "scripts": {
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:run:allure": "npx cypress run --config video=true --env allure=true",
    "cy:gererate:allure": "allure generate allure-results --clean --output allure-report",
    "cy:open:allure": "allure open allure-report"
  }
}
